              duration: 1200,
            })
    </script -->
  <body>
    <%= render partial: 'layouts/web_header' %>
    <%= render partial: 'layouts/intro_section' %>
    <%= render partial: 'layouts/about_us' %>
    <%= render partial: 'layouts/services_section'%>
    <%= render partial: 'layouts/why_us'%>
    <%= render partial: 'layouts/our_products'%>
    <%= render partial: 'layouts/testimonial'%>
    <%= render partial: 'layouts/our_team'%>
    <%= render partial: 'layouts/our_clients'%>
    <%= render partial: 'layouts/contact_us'%>
